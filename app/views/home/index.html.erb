

<h1>Belajar Rails</h1>
<ul>
<% @langs.each do |lang| %>
<li><%= lang %></li>
<% end %>
</ul>

<table>
    <tr>
        <th>Title</th>
        <th>Pages</th>
        <th>Description</th>
        <th>Action</th>
    </tr>
    <% @books.each do |book| %>
    <tr>
        <td><%= book.title %></td>
        <td><%= book.page %></td>
        <td><%= book.description %></td>
        <td><%= link_to 'EDIT'%> | <%= link_to 'DELETE'%></td>
    </tr>
    <% end %>
</table>
